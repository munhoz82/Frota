<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>Gerenciamento de FROTA | @ViewBag.Title</title>

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
    
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <!-- Bootstrap CSS -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    
    <!-- MetisMenu CSS (v2.7.9 for Inspinia compatibility) -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/metisMenu/2.7.9/metisMenu.min.css">
    
    <!-- Animate CSS -->
    <link href="~/Content/animate.css" rel="stylesheet" type="text/css" />
    
    <!-- Primary Inspinia style -->
    <link href="~/Content/style.css" rel="stylesheet" type="text/css" />

    @RenderSection("Styles", required: false)
</head>
<body>
    <!-- Wrapper-->
    <div id="wrapper">
        <!-- Navigation -->
        @Html.Partial("_Navigation")

        <!-- Page wraper -->
        <div id="page-wrapper" class="gray-bg">
            <!-- Top Navbar -->
            @Html.Partial("_TopNavbar")

            <!-- Main view  -->
            @RenderBody()

            <!-- Footer -->
            @Html.Partial("_Footer")
        </div>
        <!-- End page wrapper-->
    </div>
    <!-- End wrapper-->

    <!-- jQuery -->
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    
    <!-- Bootstrap -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    
    <!-- Metis Menu (v2.7.9 for Inspinia compatibility) -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/metisMenu/2.7.9/metisMenu.min.js"></script>
    
    <!-- SlimScroll -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-slimScroll/1.3.8/jquery.slimscroll.min.js"></script>
    
    <!-- Custom and plugin javascript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/inspinia/2.9.4/js/inspinia.min.js"></script>
    
    <!-- Initialize navigation -->
    <script>
        $(document).ready(function() {
            $('#side-menu').metisMenu();
        });
    </script>

    @RenderSection("Scripts", required: false)
</body>
</html>
